{
  "name": "@topolop/analysis",
  "version": "0.1.0",
  "description": "Analysis engine for Topolop - integrates 29+ static analysis tools with cross-tool correlation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "restricted",
    "registry": "http://localhost:4873"
  },
  "bin": {
    "topolop-analysis": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "topolop",
    "analysis",
    "static-analysis",
    "code-quality",
    "security",
    "correlation"
  ],
  "author": "cordlesssteve",
  "license": "MIT",
  "dependencies": {
    "@topolop/shared-types": "*"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.12.2",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cordlesssteve/topolop-analysis.git"
  }
}
