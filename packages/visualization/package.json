{
  "name": "@topolop/visualization",
  "version": "0.1.0",
  "description": "3D visualization engine for Topolop - renders code analysis as interactive city metaphors",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "restricted",
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "serve": "http-server ./dist -p 8080",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "topolop",
    "visualization",
    "3d",
    "threejs",
    "webgl",
    "city-metaphor"
  ],
  "author": "cordlesssteve",
  "license": "MIT",
  "dependencies": {
    "@topolop/shared-types": "*",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "http-server": "^14.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cordlesssteve/topolop-visualization.git"
  }
}
